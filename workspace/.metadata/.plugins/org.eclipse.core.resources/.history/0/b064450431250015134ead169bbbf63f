public class Ifelse {
	
public static void main(String args[]) {
	int value=3;
	
	if (value>=3) {
		
	System.out.println("Hello");
	}
	
	else if (value>=2) {
	
	System.out.println("Hi"); }
	
	else if (value>=1) {
	
	 System.out.println("Good");}
	
	else;
	System.out.println("No Match Found");
	
	
	

	
		


	
}

}
		


